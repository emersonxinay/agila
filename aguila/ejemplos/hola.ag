imprimir "Hola desde √ÅGUILA"

nombre = "Emerson"
edad = 25
activo = verdadero

numeros = [10, 20, 30]

funcion mostrar() {
    imprimir nombre
    imprimir edad
}

si activo {
    imprimir "Usuario activo"
} sino {
    imprimir "Usuario inactivo"
}

para i = 0 hasta 3 {
    imprimir i
}

para n en numeros {
    imprimir n
}

clase Persona {
    nombre
    edad: numero

    funcion saludar() {
        imprimir "Hola, soy " + self.nombre
    }
}

juan = Persona()
juan.nombre = "Juan"
juan.edad = 30
juan.saludar()

ana = Persona()
ana.nombre = "Ana"
ana.edad = 25
ana.saludar()

mostrar()
