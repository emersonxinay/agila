funcion fib(n) {
    si (n < 2) {
        retornar n
    }
    retornar fib(n - 1) + fib(n - 2)
}

imprimir("Calculando Fibonacci de N...")
n = leer("Ingrese N (40): ");
inicio = reloj()
resultado = fib(n)
fin = reloj()

imprimir(a"Resultado: {resultado}")
duracion = fin - inicio
imprimir(a"Tiempo: {duracion} segundos")
